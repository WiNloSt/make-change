{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "jsx": "preserve",
    "strictFunctionTypes": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "checkJs": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
